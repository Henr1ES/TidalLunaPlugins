{
	"name": "@Henr1ES/TidalLuna-Plugins",
	"description": "Plugins for Tidal Luna made by @Henr1ES :D",
	"type": "module",
	"scripts": {
		"watch": "concurrently \"pnpm:build --watch\" pnpm:serve",
		"build": "rimraf ./dist && tsx esbuild.config.ts",
		"serve": "http-server ./dist -p 3000 -s --cors -c-1",
		"postinstall": "patch-package"
	},
	"devDependencies": {
		"@types/node": "^24.0.0",
		"@types/react": "^19.1.7",
		"@types/react-dom": "^19.1.6",
